{
  "name": "speed-games",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "esbuild scripts/index.js --bundle --outfile=dist/bundle.js && uglifyjs dist/bundle.js --compress --output dist/bundle.js",
    "build-watch": "esbuild scripts/index.js --bundle --outfile=dist/bundle.js --watch",
    "server": "http-server -c-1 -p 0 -a localhost -o",
    "start": "npm run build && npm run server",
    "start:dev": "npm-run-all --parallel build-watch server",
    "upgrade": "npx npm-check -u && npm install && npm update && npm prune && npm audit fix"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@yuigoto/infiniship-js": "^1.0.0",
    "confetti-js": "^0.0.18",
    "csv-parse": "^5.4.0",
    "js-confetti": "^0.11.0",
    "lodash": "^4.17.21",
    "papaparse": "^5.4.1"
  },
  "devDependencies": {
    "esbuild": "^0.18.10",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "uglify-js": "^3.17.4"
  }
}
